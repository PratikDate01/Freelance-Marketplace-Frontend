{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/client/build/static/$1" },
    { "src": "/favicon.ico", "dest": "/client/build/favicon.ico" },
    { "src": "/manifest.json", "dest": "/client/build/manifest.json" },
    { "src": "/asset-manifest.json", "dest": "/client/build/asset-manifest.json" },
    { "src": "/logo192.png", "dest": "/client/build/logo192.png" },
    { "src": "/logo512.png", "dest": "/client/build/logo512.png" },
    { "src": "/robots.txt", "dest": "/client/build/robots.txt" },
    { "src": "/(.*)", "dest": "/client/build/index.html" }
  ],
  "env": {
    "REACT_APP_API_URL": "https://freelance-marketplace-backend-bppe.onrender.com",
    "REACT_APP_PRODUCTION_API_URL": "https://freelance-marketplace-backend-bppe.onrender.com"
  }
}